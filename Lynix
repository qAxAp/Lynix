	char.ChildAdded:Connect(function(child)
		if child:IsA("Tool") then
			task.wait(0.1)
			applyFastReloadToTool(child)
		end
	end)
	scanFastReload(char)
end

local charAddedFastConn = player.CharacterAdded:Connect(hookCharacterFast)
