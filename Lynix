local Weapons = {
	["M9"] = true,
	["MP5"] = true,
	["Remington 870"] = true,
	["AK-47"] = true,
	["M4A1"] = true,
	["FAL"] = true,
	["Taser"] = true,
}

local ZeroRecoilEnabled = false
local OriginalSpread = {}
local ZeroRecoilAttrConn = {}

local function ensureZeroRecoilAttributeWatcher(tool)
	if ZeroRecoilAttrConn[tool] then return end
	ZeroRecoilAttrConn[tool] = tool:GetAttributeChangedSignal("SpreadRadius"):Connect(function()
		if ZeroRecoilEnabled then
			tool:SetAttribute("SpreadRadius", 0)
		end
	end)
end

local function applyZeroRecoilToTool(tool)
	if not tool or not tool:IsA("Tool") then return end
	if not Weapons[tool.Name] then return end

	local current = tool:GetAttribute("SpreadRadius")
	if current == nil then return end

	if OriginalSpread[tool] == nil then
		OriginalSpread[tool] = current
	end

	if ZeroRecoilEnabled then
		tool:SetAttribute("SpreadRadius", 0)
	else
		if OriginalSpread[tool] ~= nil then
			tool:SetAttribute("SpreadRadius", OriginalSpread[tool])
		end
	end

	ensureZeroRecoilAttributeWatcher(tool)
end

local function scanZeroRecoil(character)
	character = character or player.Character
	if not character then return end

	for _, t in ipairs(character:GetChildren()) do
		if t:IsA("Tool") then
			applyZeroRecoilToTool(t)
		end
	end
	for _, t in ipairs(backpack:GetChildren()) do
		if t:IsA("Tool") then
			applyZeroRecoilToTool(t)
		end
	end
end

local function hookCharacterZero(char)
	if not char then return end
	char.ChildAdded:Connect(function(child)
		if child:IsA("Tool") then
			task.wait(0.1)
			applyZeroRecoilToTool(child)
		end
	end)
	scanZeroRecoil(char)
end

local charAddedZeroConn = player.CharacterAdded:Connect(hookCharacterZero)
if player.Character then hookCharacterZero(player.Character) end

local backpackAddedZeroConn = backpack.ChildAdded:Connect(function(child)
	if child:IsA("Tool") then
		task.wait(0.1)
		applyZeroRecoilToTool(child)
	end
end)

TextButton.MouseButton1Click:Connect(function()
	ZeroRecoilEnabled = not ZeroRecoilEnabled
	if ZeroRecoilEnabled then
		Frame_6.BackgroundTransparency = 0
		TextButton.BackgroundTransparency = 0
	else
		Frame_6.BackgroundTransparency = 0.93
		TextButton.BackgroundTransparency = 1
	end
	scanZeroRecoil()
end)

local RapidFireEnabled = false
local OriginalFireRate = {}
local OriginalAutoFire = {}
local RapidFireFireConn = {}
local RapidFireAutoConn = {}

local function ensureRapidFireWatchers(tool)
	if not RapidFireFireConn[tool] then
		RapidFireFireConn[tool] = tool:GetAttributeChangedSignal("FireRate"):Connect(function()
			if RapidFireEnabled then
				tool:SetAttribute("FireRate", 0.04)
			end
		end)
	end
	if not RapidFireAutoConn[tool] then
		RapidFireAutoConn[tool] = tool:GetAttributeChangedSignal("AutoFire"):Connect(function()
			if RapidFireEnabled then
				tool:SetAttribute("AutoFire", true)
			end
		end)
	end
end

local function applyRapidFireToTool(tool)
	if not tool or not tool:IsA("Tool") then return end
	if not Weapons[tool.Name] then return end

	local currentFire = tool:GetAttribute("FireRate")
	if currentFire ~= nil and OriginalFireRate[tool] == nil then
		OriginalFireRate[tool] = currentFire
	end

	local currentAuto = tool:GetAttribute("AutoFire")
	if currentAuto ~= nil and OriginalAutoFire[tool] == nil then
		OriginalAutoFire[tool] = currentAuto
	end

	if RapidFireEnabled then
		if currentFire ~= nil then tool:SetAttribute("FireRate", 0.04) end
		if currentAuto ~= nil then tool:SetAttribute("AutoFire", true) end
	else
		if OriginalFireRate[tool] ~= nil then
			tool:SetAttribute("FireRate", OriginalFireRate[tool])
		end
		if OriginalAutoFire[tool] ~= nil then
			tool:SetAttribute("AutoFire", OriginalAutoFire[tool])
		end
	end

	ensureRapidFireWatchers(tool)
end

local function scanRapidFire(character)
	character = character or player.Character
	if not character then return end

	for _, t in ipairs(character:GetChildren()) do
		if t:IsA("Tool") then
			applyRapidFireToTool(t)
		end
	end
	for _, t in ipairs(backpack:GetChildren()) do
		if t:IsA("Tool") then
			applyRapidFireToTool(t)
		end
	end
end

local function hookCharacterRapid(char)
	if not char then return end
	char.ChildAdded:Connect(function(child)
		if child:IsA("Tool") then
			task.wait(0.1)
			applyRapidFireToTool(child)
		end
	end)
	scanRapidFire(char)
end

local charAddedRapidConn = player.CharacterAdded:Connect(hookCharacterRapid)
if player.Character then hookCharacterRapid(player.Character) end

local backpackAddedRapidConn = backpack.ChildAdded:Connect(function(child)
	if child:IsA("Tool") then
		task.wait(0.1)
		applyRapidFireToTool(child)
	end
end)

TextButton_2.MouseButton1Click:Connect(function()
	RapidFireEnabled = not RapidFireEnabled
	if RapidFireEnabled then
		Frame_8.BackgroundTransparency = 0
		TextButton_2.BackgroundTransparency = 0
	else
		Frame_8.BackgroundTransparency = 0.93
		TextButton_2.BackgroundTransparency = 1
	end
	scanRapidFire()
end)

local FastReloadEnabled = false
local OriginalReloadTime = {}
local FastReloadConn = {}

local function ensureFastReloadWatcher(tool)
	if FastReloadConn[tool] then return end
	FastReloadConn[tool] = tool:GetAttributeChangedSignal("ReloadTime"):Connect(function()
		if FastReloadEnabled then
			tool:SetAttribute("ReloadTime", 0)
		end
	end)
end

local function applyFastReloadToTool(tool)
	if not tool or not tool:IsA("Tool") then return end
	if not Weapons[tool.Name] then return end

	local current = tool:GetAttribute("ReloadTime")
	if current ~= nil and OriginalReloadTime[tool] == nil then
		OriginalReloadTime[tool] = current
	end

	if FastReloadEnabled then
		if current ~= nil then tool:SetAttribute("ReloadTime", 0) end
	else
		if OriginalReloadTime[tool] ~= nil then
			tool:SetAttribute("ReloadTime", OriginalReloadTime[tool])
		end
	end

	ensureFastReloadWatcher(tool)
end

local function scanFastReload(character)
