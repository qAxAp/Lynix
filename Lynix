local function addHoverEffect(guiObject, stroke, baseThickness)
	baseThickness = baseThickness or 0.7
	local scale = Instance.new("UIScale")
	scale.Parent = guiObject
	scale.Scale = 1

	local info = TweenInfo.new(0.15, Enum.EasingStyle.Quint, Enum.EasingDirection.Out)
	local hoverScale = TweenService:Create(scale, info, {Scale = 1.03})
	local leaveScale = TweenService:Create(scale, info, {Scale = 1})

	local hoverStroke, leaveStroke
	if stroke then
		hoverStroke = TweenService:Create(stroke, info, {Thickness = baseThickness + 0.5})
		leaveStroke = TweenService:Create(stroke, info, {Thickness = baseThickness})
	end

	guiObject.MouseEnter:Connect(function()
		hoverScale:Play()
		if hoverStroke then hoverStroke:Play() end
	end)

	guiObject.MouseLeave:Connect(function()
		leaveScale:Play()
		if leaveStroke then leaveStroke:Play() end
	end)
end

addHoverEffect(Tab1, Tab1Stroke, 0.7)
addHoverEffect(Tab2, Tab2Stroke, 0.7)
addHoverEffect(Tab3, Tab3Stroke, 0.7)

addHoverEffect(Frame_5, Frame5Stroke, 0.7)
addHoverEffect(Frame_7, Frame7Stroke, 0.7)
addHoverEffect(Frame_9, Frame9Stroke, 0.7)
addHoverEffect(Frame_11, Frame11Stroke, 0.7)
addHoverEffect(Frame_13, Frame13Stroke, 0.7)
addHoverEffect(Frame_15, Frame15Stroke, 0.7)
addHoverEffect(WalkSpeedFrame, WalkSpeedStroke, 0.7)
addHoverEffect(JumpPowerFrame, JumpPowerStroke, 0.7)
addHoverEffect(InfiniteJumpFrame, InfiniteJumpStroke, 0.7)
addHoverEffect(NoclipFrame, NoclipStroke, 0.7)
addHoverEffect(ESPEnv.Frame, ESPEnv.FrameStroke, 0.7)

local TeleportCoordinates = {
	Armory = Vector3.new(828.2183837890625, 99.97666931152344, 2246.656005859375),
	Kitchen = Vector3.new(772.7656860351562, 99.98999786376953, 2263.6357421875),
	["Café"] = Vector3.new(914.18994140625, 99.98995208740234, 2322.803955078125),
	Nexus = Vector3.new(880.0348510742188, 99.99999237060547, 2388.252197265625),
	Cellblock = Vector3.new(915.101806640625, 99.98998260498047, 2438.829833984375),
	["Criminal base"] = Vector3.new(-921.727294921875, 94.12886810302734, 2062.712158203125),
}

local function teleportToCoord(name)
	local pos = TeleportCoordinates[name]
	if not pos then return end

	local character = player.Character or player.CharacterAdded:Wait()
	local hrp = character:FindFirstChild("HumanoidRootPart")
	if not hrp then return end

	hrp.CFrame = CFrame.new(pos + Vector3.new(0, 3, 0))
end

local teleportButtonsData = {
	"Armory",
	"Kitchen",
	"Café",
	"Nexus",
	"Cellblock",
	"Criminal base",
}

for _, name in ipairs(teleportButtonsData) do
	local Row = Instance.new("Frame")
	Row.Parent = TeleportScroll
	Row.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
	Row.BorderColor3 = Color3.fromRGB(0, 0, 0)
	Row.BorderSizePixel = 0
	Row.Size = UDim2.new(1, -20, 0, 38)

	local RowCorner = Instance.new("UICorner")
	RowCorner.CornerRadius = UDim.new(0, 3)
	RowCorner.Parent = Row

	local RowStroke = Instance.new("UIStroke")
	RowStroke.Parent = Row
	RowStroke.Thickness = 0.7
	RowStroke.Color = Color3.fromRGB(255, 217, 78)
	RowStroke.ApplyStrokeMode = Enum.ApplyStrokeMode.Border

	local RowLabel = Instance.new("TextLabel")
	RowLabel.Parent = Row
	RowLabel.BackgroundTransparency = 1.000
	RowLabel.BorderSizePixel = 0
	RowLabel.Position = UDim2.new(0.05, 0, 0, 0)
	RowLabel.Size = UDim2.new(0, 200, 0, 37)
	RowLabel.Font = Enum.Font.FredokaOne
	RowLabel.Text = name
	RowLabel.TextColor3 = Color3.fromRGB(235, 255, 103)
	RowLabel.TextSize = 14.000
	RowLabel.TextXAlignment = Enum.TextXAlignment.Left

	local RowButton = Instance.new("ImageButton")
	RowButton.Parent = Row
	RowButton.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	RowButton.BackgroundTransparency = 1.000
	RowButton.BorderColor3 = Color3.fromRGB(0, 0, 0)
	RowButton.BorderSizePixel = 0
	RowButton.Position = UDim2.new(0.876666486, 0, 0.131578952, 0)
	RowButton.Size = UDim2.new(0, 28, 0, 28)
	RowButton.Image = "rbxassetid://110620726041573"

	RowButton.MouseButton1Click:Connect(function()
		teleportToCoord(name)
	end)

	addHoverEffect(Row, RowStroke, 0.7)
end

do
	local dragging = false
	local dragInput, dragStart, startPos

	local function update(input)
		local delta = input.Position - dragStart
		Frame.Position = UDim2.new(
			startPos.X.Scale,
			startPos.X.Offset + delta.X,
			startPos.Y.Scale,
			startPos.Y.Offset + delta.Y
		)
	end

	Frame.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			local relY = input.Position.Y - Frame.AbsolutePosition.Y
			if relY <= 40 then
				dragging = true
				dragStart = input.Position
				startPos = Frame.Position

				input.Changed:Connect(function()
					if input.UserInputState == Enum.UserInputState.End then
						dragging = false
					end
				end)
			end
		end
	end)

	Frame.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement
			or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)

	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end

local pages = {
	Tab1 = ScrollingFrame,
	Tab2 = TeleportPage,
	Tab3 = PlayerPage,
}

local buttons = {
	Tab1 = Tab1,
	Tab2 = Tab2,
	Tab3 = Tab3,
}

local function setTab(tabName)
	for name, page in pairs(pages) do
		page.Visible = (name == tabName)
	end
	for name, btn in pairs(buttons) do
		if name == tabName then
			btn.BackgroundColor3 = Color3.fromRGB(255, 217, 78)
			btn.TextColor3 = Color3.fromRGB(9, 9, 9)
		else
			btn.BackgroundColor3 = Color3.fromRGB(12, 12, 12)
			btn.TextColor3 = Color3.fromRGB(235, 255, 103)
		end
	end
end

Tab1.MouseButton1Click:Connect(function() setTab("Tab1") end)
Tab2.MouseButton1Click:Connect(function() setTab("Tab2") end)
Tab3.MouseButton1Click:Connect(function() setTab("Tab3") end)
setTab("Tab1")

